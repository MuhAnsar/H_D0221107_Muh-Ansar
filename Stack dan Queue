print ('_'*17,' STACK','_'*17)


def buat_stack ():
    stack = ['Celana','Topi','kecamata']
    return stack
def check_empty (stack):
    return len(stack) == 0
def push(stack,item):
    stack.append (item)
    print ('Barang Yang Ditambahkan: ',item)
def pop(stack):
    print ('Barang Yang Dikeluarkan: ',stack.pop())
def size(stack):
    print ('Jumlah Barang Yang Ada Di Stack: ',len(stack))
def top(stack):
    top = len (stack) - 1
    if top < 0:
        print ('Tidak Terdefinisi')
    else:
        print ('Barang Paling Teratas:',stack[top])
def tampilkan (stack):
    print (stack)

s = buat_stack()
print ('Barang Yang Ditumpuk: ',s)
pop(s)
top(s)
push(s,'Baju')
print (s)
size(s)
check_empty(s)
print()


print ('_'*17,'QUEUE','_'*17)

Queue = []
Antrian = input('Masukkan Antrian: ')
def isEmpty():
    if len(Queue) == 0:
        print ('Queue Kosong')
        return True
    return False
def isFull():
    if len(Queue) == Antrian:
        print ('Queue Penuh')
        return True
    return False
def dequeue():
    if isEmpty() == False:
        print (Queue.pop(0),'Terhapus Dari Antrian')
def enqueue():
    if isFull() == False:
        x = input('Masukkan Antrian Baru: ')
        Queue.append(x)
        print (x,'Ditambahkan Ke Antrian')
print(Antrian)
Tugas = 0
while Tugas != '6':
    print('''1.Enqueue = Menambah Antrian
    2.Dequeue = Menghapus Antrian
    3.Full = Memeriksa Apakah Antrian Penuh
    4.Size = Melihat Ukuran Antrian''')
    Tugas = input('''Ketik 6 Untuk Berhenti
    Masukkan Perintah: ''')
    print()
    if Tugas == '1':
        enqueue()
    elif Tugas == '2':
        dequeue()
    elif Tugas == '3':
        if isFull() == False:
            print('Antrian Belum Penuh')
    elif Tugas == '4':
        print ('Ukuran Antrian Saat Ini: ',len(Queue),'\nUkuran Maksimal Antrian: ',Antrian)